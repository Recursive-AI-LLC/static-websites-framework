template engine only if content is not already pre-rendered\nasync function initTemplates() {\n    // Check if content is already pre-rendered (static site)\n    const appElement = document.querySelector('#app');\n    \n    // If the app element has substantial content, it's already pre-rendered\n    if (appElement && appElement.innerHTML.trim().length > 50) {\n        console.log('Content already pre-rendered, skipping client-side rendering');\n        return;\n    }\n    \n    console.log('No pre-rendered content found, initializing client-side templates');\n    \n    // Register all partials first\n    await registerAllPartials();\n\n    // Get the current page path\n    const pagePath = window.location.pathname;\n\n    // Determine which template to load based on the path\n    // This is a simple routing mechanism\n    let templatePath;\n    let pageData = {};\n\n    if (pagePath === '/' || pagePath === '/index.html') {\n        templatePath = '/src/pages/home.html';\n        pageData = { title: 'Home Page' };\n    } else {\n        // Extract page name from path\n        const pageName = pagePath.split('/').pop().replace('.html', '');\n        templatePath = `/src/pages/${pageName}.html`;\n        pageData = { title: pageName.charAt(0).toUpperCase() + pageName.slice(1) };\n    }\n\n    // Render the page template\n    await renderTemplate('#app', templatePath, pageData);\n}\n\n// Initialize when the DOM is ready\ndocument.addEventListener('DOMContentLoaded', initTemplates);"},"Content":null,"IsError":false,"Id":"toolu_012SkfS7fTiCVYBfYnMWh8AW","IsCached":false}],"TokenCount":522},{"Id":"pdMWEU","Role":0,"Content":[{"Type":"tool","Tool":"WriteFile","Input":null,"Content":[{"Text":"Wrote to file src/js/main.js","Type":"text","Id":null,"IsCached":false}],"IsError":false,"Id":"toolu_012SkfS7fTiCVYBfYnMWh8AW","IsCached":false}],"TokenCount":65},{"Id":"JjufHT","Role":1,"Content":[{"Type":"tool","Tool":"ExecuteTerminalCommand","Input":{"Reasoning":"Now let me rebuild 